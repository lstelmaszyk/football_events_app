{
  "name": "recruitment/football-events",
  "description": "Simple football events tracking application",
  "type": "project",
  "require": {
    "php": ">=8.4",
    "php-di/php-di": "^7.1",
    "symfony/routing": "^7.3",
    "symfony/http-foundation": "^7.3",
    "enqueue/fs": "^0.10.26",
    "symfony/console": "^7.3",
    "ext-pcntl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "codeception/codeception": "^5.0",
    "codeception/module-phpbrowser": "*",
    "codeception/module-asserts": "*",
    "codeception/module-rest": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
